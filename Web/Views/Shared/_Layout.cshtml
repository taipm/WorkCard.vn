    <!--- My Script-->
    @*@Styles.Render("~/Content/css")*@
    @*@Styles.Render("~/Content/themes/base/css")*@

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/jqueryui")

    @*<script src="~/Scripts/jquery-1.8.2.min.js"></script>*@
    <script src="~/Scripts/jquery-3.1.1.min.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>

    <script src="~/Scripts/ckeditor/ckeditor.js"></script>

    <script type="text/x-mathjax-config">
        MathJax.Hub.Config({
        tex2jax: { inlineMath: [['$','$'],['\\(','\\)']] }
        });
    </script>
    <script type='text/javascript' src='http://cdn.mathjax.org/mathjax/latest/MathJax.js?config=TeX-AMS-MML_HTMLorMML'></script>
</head>

<body>
    <!-- Navigation Bar-->
    <header id="topnav">
        @RenderPage("~/Views/Shared/Templates/_Top.cshtml")
        @RenderPage("~/Views/Shared/Templates/_Menu.cshtml")
    </header>
    <!-- End Navigation Bar-->

    <div class="wrapper">
        <div class="container">
            <!-- Page-Title -->
            @RenderPage("~/Views/Shared/Templates/_Setting.cshtml")

            @RenderBody()

            <!-- Footer -->
            @RenderPage("~/Views/Shared/Templates/_Footer.cshtml")
            <!-- End Footer -->
        </div>
        <!-- end container -->
        <!-- Right Sidebar -->
        @RenderPage("~/Views/Shared/Templates/_Sidebar.cshtml")
        <!-- /Right-bar -->
    </div>

    <!-- jQuery  -->
    <script src="~/Assets/Templates/Adminto/js/jquery.min.js"></script>
    <script src="~/Assets/Templates/Adminto/js/bootstrap.min.js"></script>
    <script src="~/Assets/Templates/Adminto/js/detect.js"></script>
    <script src="~/Assets/Templates/Adminto/js/fastclick.js"></script>
    <script src="~/Assets/Templates/Adminto/js/jquery.slimscroll.js"></script>
    <script src="~/Assets/Templates/Adminto/js/jquery.blockUI.js"></script>
    <script src="~/Assets/Templates/Adminto/js/waves.js"></script>
    <script src="~/Assets/Templates/Adminto/js/wow.min.js"></script>
    <script src="~/Assets/Templates/Adminto/js/jquery.nicescroll.js"></script>
    <script src="~/Assets/Templates/Adminto/js/jquery.scrollTo.min.js"></script>

    <!-- App js -->
    <script src="~/Assets/Templates/Adminto/js/jquery.core.js"></script>
    <script src="~/Assets/Templates/Adminto/js/jquery.app.js"></script>

    @RenderSection("scripts", required: false)
</body>
</html>